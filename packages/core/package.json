{
  "name": "@repo/core",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.12.0 <23"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js",
      "default": "./dist/auth/index.js"
    },
    "./cookies": {
      "types": "./dist/cookies/index.d.ts",
      "import": "./dist/cookies/index.js",
      "default": "./dist/cookies/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js",
      "default": "./dist/services/index.js"
    },
    "./stripe": {
      "types": "./dist/stripe/index.d.ts",
      "import": "./dist/stripe/index.js",
      "default": "./dist/stripe/index.js"
    },
    "./email": {
      "types": "./dist/email/index.d.ts",
      "import": "./dist/email/index.js",
      "default": "./dist/email/index.js"
    },
    "./analytics": {
      "types": "./dist/services/analytics.d.ts",
      "import": "./dist/services/analytics.js",
      "default": "./dist/services/analytics.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage --threshold=70"
  },
  "dependencies": {
    "@repo/database": "workspace:*",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "slugify": "^1.6.6",
    "nanoid": "^4.0.2"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "eslint": "^9.31.0",
    "tsup": "^8.0.0",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts"
}