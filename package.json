{
  "name": "with-svelte",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter web",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "check": "turbo run check",
    "db:types": "npx supabase gen types typescript --project-id koowfhsaqmarfdkwsfiz --schema public > packages/database/src/generated.ts",
    "db:types:local": "npx supabase gen types typescript --local > packages/database/src/generated.ts"
  },
  "devDependencies": {
    "prettier": "^3.6.0",
    "prettier-plugin-svelte": "^3.4.0",
    "supabase": "^2.0.0",
    "turbo": "2.5.4"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@stripe/stripe-js": "^7.8.0"
  }
}
