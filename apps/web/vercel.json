{
  "framework": "sveltekit",
  "buildCommand": "pnpm build",
  "outputDirectory": ".svelte-kit",
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev",
  "functions": {
    "apps/web/build/index.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}