{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://driplo.xyz/bg/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "bg.driplo.xyz"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "https://driplo.xyz/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "uk.driplo.xyz"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "https://driplo.xyz/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "en.driplo.xyz"
        }
      ]
    }
  ]
}